import Foundation

open class Api {
{{#each methods}}
    open static func {{name}}(
    {{#each params}}
    {{name}}: {{type}},
    {{/each}}
    completion: (Error?, {{response.type}}) -> Void
    ) {
        // {{method}} {{path}}
    }
{{/each}}
}


{{#each objectModels}}

  open class {{name}} {
  {{#with schema}}
    {{#each properties}}
      public var {{@key}}: {{type}}
    {{/each}}

    public init(
    {{#each properties}}
      {{@key}}: {{type}}{{#unless @last}},{{/unless}}
    {{/each}}
    ) {
    {{#each properties}}
      self.{{@key}} = {{@key}}
    {{/each}}
    }
  {{/with}}
  }

{{/each}}

{{#each enumModels}}

  public enum {{name}}: {{schema.enumType}} {
  {{#with schema}}
    {{#each values}}
      case {{name}} = {{{value}}}
    {{/each}}
  {{/with}}
  }

{{/each}}
